<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape>
            <solid android:color="@color/white" />
            <stroke
                android:dashGap="5dp"
                android:dashWidth="7dp"
                android:width="2dp"
                android:color="@color/custom_green" />
            <padding
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp" />
            <corners android:radius="16dp" />
        </shape>
    </item>
</layer-list>